{
  "name": "nodets-sample",
  "version": "1.0.0",
  "description": "",
  "main": "dist/src/index.js",
  "dependencies": {
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "cls-hooked": "^4.2.2",
    "cors": "2.8.5",
    "debug": "^4.1.1",
    "express": "4.17.1",
    "express-openapi-validator": "^3.12.7",
    "js-yaml": "^3.13.1",
    "node-uuid": "^1.4.8",
    "perfy": "^1.1.5",
    "prom-client": "12.0.0",
    "swagger-ui-express": "^4.1.4",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/chai": "4.2.11",
    "@types/chai-http": "4.2.0",
    "@types/cls-hooked": "^4.3.0",
    "@types/debug": "4.1.5",
    "@types/express": "4.17.6",
    "@types/js-yaml": "^3.12.5",
    "@types/mocha": "7.0.2",
    "@types/nock": "^11.1.0",
    "@types/node": "12.7.11",
    "@types/node-uuid": "0.0.28",
    "@types/perfy": "^1.1.0",
    "@types/swagger-ui-express": "^4.1.2",
    "chai": "4.2.0",
    "chai-http": "^4.3.0",
    "cpx": "1.5.0",
    "debug-level": "^0.3.1",
    "del-cli": "^3.0.1",
    "mocha": "7.1.2",
    "nock": "^12.0.3",
    "nodemon": "^2.0.3",
    "sinon": "^9.0.2",
    "supertest": "4.0.2",
    "ts-mocha": "7.0.0",
    "ts-node": "8.10.1",
    "tslint": "6.1.2",
    "typescript": "3.8.3"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "clean:windows": "del-cli dist",
    "build:copy": "cpx \"src/**/*.yaml\" dist/",
    "build:live": "npm run clean && npm run lint && tsc -p tsconfig.json && npm run build:copy",
    "build:live:windows": "npm run clean:windows && npm run lint && tsc -p tsconfig.json && npm run build:copy",
    "build:dev": "nodemon --exec ts-node --ext ts,js,json -- ./src/index.ts",
    "dev": "export DEBUG=app:* && export TS_NODE_CACHE=false && npm run build:dev",
    "dev:windows": "set DEBUG=app:* && set TS_NODE_CACHE=false && npm run build:dev",
    "install:start": "npm install && npm run test:noDebug && npm run start:live",
    "install:start:windows": "npm install && npm run test:windows:noDebug && npm run start:live:windows",
    "lint": "tslint -p tsconfig.json",
    "start": "export DEBUG=app:* && node dist/index.js",
    "start:windows": "set DEBUG=app:* && node dist/index.js",
    "start:live": "npm run build:live && export DEBUG=app:* && node dist/index.js",
    "start:live:windows": "npm run build:live:windows && set DEBUG=app:* && node dist/index.js",
    "test": "export DEBUG=app:* && export TS_NODE_CACHE=false && ./node_modules/.bin/mocha --require ts-node/register --recursive './test/**/*.spec.ts' --exit",
    "test:noDebug": "export DEBUG=NONE && export TS_NODE_CACHE=false && ./node_modules/.bin/mocha --require ts-node/register --recursive './test/**/*.spec.ts' --exit",
    "test:windows": "set DEBUG=app:* && set TS_NODE_CACHE=false && .\\node_modules\\.bin\\mocha --require ts-node/register --recursive './test/**/*.spec.ts' --exit",
    "test:windows:noDebug": "set DEBUG=NONE && set TS_NODE_CACHE=false && .\\node_modules\\.bin\\mocha --require ts-node/register --recursive './test/**/*.spec.ts' --exit",
    "testApp": "export TS_NODE_CACHE=false && ./node_modules/.bin/mocha --require ts-node/register './test/application.spec.ts'",
    "testApp:windows": "set TS_NODE_CACHE=false && .\\node_modules\\.bin\\mocha --require ts-node/register '.\\test\\application.spec.ts'"
  },
  "engines": {
    "node": ">6.11.0"
  },
  "author": "RHAPPDEV",
  "license": "ISC"
}
